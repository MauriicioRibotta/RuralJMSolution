{
  "name": "rural-ia-web",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "watch:css": "npx @tailwindcss/cli -i ./src/styles.css -o ./src/output.css --watch",
    "start": "concurrently \"npm run watch:css\" \"ng serve\"",
    "build": "npx @tailwindcss/cli -i ./src/styles.css -o ./src/output.css --minify && ng build",
    "watch": "concurrently \"npm run watch:css\" \"ng build --watch --configuration development\"",
    "test": "npx @tailwindcss/cli -i ./src/styles.css -o ./src/output.css && ng test"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "packageManager": "npm@11.7.0",
  "dependencies": {
    "@angular/common": "^21.1.0",
    "@angular/compiler": "^21.1.0",
    "@angular/core": "^21.1.0",
    "@angular/forms": "^21.1.0",
    "@angular/platform-browser": "^21.1.0",
    "@angular/router": "^21.1.0",
    "@supabase/supabase-js": "^2.95.3",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "^0.16.0"
  },
  "devDependencies": {
    "@angular/build": "^21.1.3",
    "@angular/cli": "^21.1.3",
    "@angular/compiler-cli": "^21.1.0",
    "@tailwindcss/postcss": "^4.1.18",
    "autoprefixer": "^10.4.24",
    "concurrently": "^9.2.1",
    "jsdom": "^27.1.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.18",
    "typescript": "~5.9.2",
    "vitest": "^4.0.8"
  }
}